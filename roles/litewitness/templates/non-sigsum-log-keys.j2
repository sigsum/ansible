{% if litewitness_vkey_log_keys is defined %}
{% for k in litewitness_vkey_log_keys %}
{{ k.vkey }} {{ k.origin }}
{% endfor %}
{% endif %}
{% for log in litewitness_logs %}
{% if log.origin is defined and log.origin != "" %}
{{ log.vkey }} {{ log.origin }}
{% else %}
{{ log.vkey }} {{ log.vkey | split('+') | first }}
{% endif %}
{% endfor %}
